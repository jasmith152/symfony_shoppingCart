{% extends 'default/base.html.twig' %}
{% block body %}
    <div class="container">
        {%if error is empty %}
            <p>The print has been added to your shopping cart.
            <a href="{{path('browsePrints')}}"><input type="button" name="continue" value="Continue Shopping"></a>
            <a href="{{path('viewCart')}}"><input type="button" name="view_cart" value="View Cart" /></a>
            </p>
        {% else %}
            {{error}}
        {% endif %}
    </div>
{% endblock %}